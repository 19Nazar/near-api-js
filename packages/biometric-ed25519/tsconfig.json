{
  "compilerOptions": {
    "esModuleInterop": false,
    "lib": [
        "es2015",
        "esnext",
        "dom"
    ],
    "module": "commonjs",
    "target": "es2015",
    "moduleResolution": "node",
    "alwaysStrict": true,
    "outDir": "./lib",
    "declaration": true,
    "preserveSymlinks": false,
    "preserveWatchOutput": true,
    "pretty": false,
    "forceConsistentCasingInFileNames": true,
    "noFallthroughCasesInSwitch": true,
    "noImplicitAny": false,
    "noImplicitReturns": true,
    "noUnusedLocals": true,
    "experimentalDecorators": true,
    "resolveJsonModule": true,
    "allowJs": true
},
"files": [
  "src/index.d.ts",
  "src/index.ts",
  "src/fido2.js",
  "src/utils.ts",
  "node_modules"
//   "node_modules/@hexagon/base64/dist/base64.min.js",
//   "node_modules/fido2-lib/dist/main.js"
],
}
